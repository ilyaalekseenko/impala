{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-zip": "*",
        "barryvdh/laravel-dompdf": "^2.2.0",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "doctrine/dbal": "^3.8",
        "dompdf/dompdf": "^2.0",
        "gotenberg/gotenberg-php": "^1.1",
        "guzzlehttp/guzzle": "^7.8",
        "hflabs/dadata": "^22.3",
        "intervention/image": "^2.7",
        "kwn/number-to-words": "^2.9",
        "laravel/framework": "^v11.0",
        "laravel/sanctum": "^v4.0",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.5.2",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "^8.2",
        "nesbot/carbon": "^2.72.5",
        "niklasravnsborg/laravel-pdf": "^4.1",
        "php-http/guzzle7-adapter": "^1.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "phpoffice/phpword": "^0.18.3",
        "psr/simple-cache": "^2.0",
        "pusher/pusher-php-server": "^7.2",
        "silverslice/excel-template": "^0.1.0",
        "tecnickcom/tcpdf": "^6.7.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.13.5",
        "fakerphp/faker": "^1.23.1",
        "laravel/sail": "^1.30.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
